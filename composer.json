{
    "name": "benjamincrozat/lumen-boilerplate",
    "description": "Quick start your next Lumen project.",
    "keywords": [
        "api",
        "boilerplate",
        "framework",
        "laravel",
        "lumen",
        "restful"
    ],
    "license": "WTFPL",
    "type": "project",
    "require": {
        "php": ">=7.1",
        "laravel/lumen-framework": "5.5.*",
        "dusterio/lumen-passport": "^0.2.4",
        "vlucas/phpdotenv": "~2.2"

    },
    "require-dev": {
        "flipbox/lumen-generator": "^5.4",
        "fzaninotto/faker": "~1.4",
        "laravel/homestead": "^6.5",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/",
            "database/"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true
    }
}
