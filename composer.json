{
    "name": "benjamincrozat/lumen-boilerplate",
    "description": "Opinionated way to start a new Lumen project.",
    "keywords": [
        "api",
        "boilerplate",
        "framework",
        "laravel",
        "lumen",
        "restful"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1",
        "laravel/lumen-framework": "5.5.*",
        "illuminate/redis": "^5.5",
        "laravel/tinker": "^1.0",
        "predis/predis": "^1.1",
        "vlucas/phpdotenv": "~2.2"
    },
    "require-dev": {
        "benjamincrozat/lumen-swagger": "^0.2.0",
        "codedungeon/phpunit-result-printer": "^0.4.4",
        "fzaninotto/faker": "~1.4",
        "itsgoingd/clockwork": "^2.0",
        "laravel/homestead": "^6.5",
        "mockery/mockery": "~1.0",
        "nunomaduro/collision": "^1.1",
        "phpunit/phpunit": "~6.0",
        "shipping-docker/vessel": "^2.1",
        "symfony/var-dumper": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/",
            "database/"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true
    }
}
